<header>
  <nav aria-label="primary">
    <ul>
      <li><a href="{{ '/' | url }}">Home</a></li>
      {% for i in collections.primary %}
      <li><a href="{{ i.url | url }}" {% if page.url==i.url %}aria-current="page"{% endif %}>{{i.data.title}}</a>
        <ul>
          {% for item in collections.secondary %}
          {% if i.fileSlug == item.data.fileslug %}
          <li><a href="{{ item.url | url }}" {% if page.url==item.url %}aria-current="page"{% endif %}>{{item.data.title}}</a></li>
          {% endif %}
          {% endfor %}
        </ul>
      </li>
      {% endfor %}
    </ul>
  </nav>
</header>